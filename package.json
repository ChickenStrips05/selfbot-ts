{
  "name": "selfbot-ts",
  "version": "1.0.0",
  "description": "A simple Discord selfbot made with TypeScript",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "@types/node": "^22.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0"
}